    <!-- http://purl.obolibrary.org/obo/ODAE_0000002 -->

    <owl:ObjectProperty rdf:about="&obo;ODAE_0000002">
        <rdfs:label xml:lang="en">drug associated with AE</rdfs:label>
        <obo:IAO_0000117>OH, SN</obo:IAO_0000117>
        <rdfs:comment>An object property that represents a relation between a drug and its associated AE.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/ODAE_0000003 -->

    <owl:ObjectProperty rdf:about="&obo;ODAE_0000003">
        <rdfs:label xml:lang="en">has disease</rdfs:label>
        <obo:IAO_0000115>An object property that represents a relation between a human and a disease.</obo:IAO_0000115>
        <obo:IAO_0000117>OH, SN</obo:IAO_0000117>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/ODAE_0000004 -->

    <owl:ObjectProperty rdf:about="&obo;ODAE_0000004">
        <rdfs:label xml:lang="en">treated with drug</rdfs:label>
        <obo:IAO_0000117>OH, SN</obo:IAO_0000117>
        <obo:IAO_0000115>An object property that represents a relation between a patient and a drug that is used to treat the patient.</obo:IAO_0000115>
    </owl:ObjectProperty>
    


    <!-- http://purl.obolibrary.org/obo/ODAE_0000005 -->

    <owl:ObjectProperty rdf:about="&obo;ODAE_0000005">
